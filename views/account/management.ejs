<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
        res.redirect('/')
    } %>

You're logged in.

<%- messages() %>

<% if (errors) { %>
    <ul class="notice">
   <% errors.array().forEach(error => { %>
     <li><%= error.msg %></li>
  <%  }) %>
   </ul>
<% } %>

<h2>Welcome <%- locals.accountData.account_firstname %></h2>
<a href="/account/edit-account/<%= locals.accountData.account_id %>">Update Account Information</a>
<% if (locals.accountData.account_type === "Admin" || locals.accountData.account_type === "Employee") { %>
    <h3>Inventory Management</h3>
    <a href="/inv/">Manage Inventory</a>
<% } %>